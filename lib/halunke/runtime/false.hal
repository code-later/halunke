(Class new 'False
  methods @[
    "and" { |'self 'other|
      self
    }
    "or" { |'self 'other|
      other
    }
    "then else" { |'self 'true_branch 'false_branch|
      (false_branch call [])
    }
    "to_b" { |'self|
      self
    }
    "to_s" { |'self 'other|
      "false"
    }
    "inspect" { |'self 'other|
      "false"
    }
  ]
)

('false = (False new))
